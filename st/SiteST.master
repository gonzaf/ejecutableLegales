            }
        });

        $(".entTab").keypress(function (event) {
            if (event.keyCode == 13) {
                event.preventDefault();
                var next = $(this).data("next");

                $("#" + next).focus();
            }
        });

        $(".soloNumeros").keypress(function (event) {

